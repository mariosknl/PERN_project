{priceRange}\n            onChange={(e) => setPriceRange(e.target.value)}\n            id=\"price_range\"\n            className=\"form-control\"\n            type=\"number\"\n          />\n        </div>\n        <button\n          type=\"submit\"\n          onClick={handleSubmit}\n          className=\"btn btn-primary\"\n        >\n          Submit\n        </button>\n      </form>\n    </div>\n  );\n};\n\nexport default UpdateRestaurant;\n","/Users/marioskanellopoulos/Sites/PERN/freecodecamp_pern/client/src/components/StarRating.jsx",[],"/Users/marioskanellopoulos/Sites/PERN/freecodecamp_pern/client/src/components/Reviews.jsx",[],"/Users/marioskanellopoulos/Sites/PERN/freecodecamp_pern/client/src/components/AddReviews.jsx",["65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":26,"column":13,"nodeType":"72","messageId":"73","endLine":26,"endColumn":21},{"ruleId":"70","severity":1,"message":"74","line":26,"column":11,"nodeType":"72","messageId":"73","endLine":26,"endColumn":28},{"ruleId":"70","severity":1,"message":"71","line":16,"column":13,"nodeType":"72","messageId":"73","endLine":16,"endColumn":21},"no-native-reassign",["75"],"no-negated-in-lhs",["76"],"no-unused-vars","'response' is assigned a value but never used.","Identifier","unusedVar","'updatedRestaurant' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]